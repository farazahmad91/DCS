    var name = nameClaim != null ? nameClaim.Value : "Guest";
    string role = User.GetLoggedInUserRole();
    //var role = User.Claims.FirstOrDefault(c => c.Type == ClaimTypes.Role);
    // //var Urole = role != null ? role.Value : "Guest";

    var ProjectName = "";
    var DomainName = "";
    var Logo = "";

    if (projectdetails.ProjectName !=null && projectdetails.DomainName !=null)
    {
        ProjectName = projectdetails.ProjectName;
        DomainName = projectdetails.DomainName;
        Logo = projectdetails.Logo;
    }

}
