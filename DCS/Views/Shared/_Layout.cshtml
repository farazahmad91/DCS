@using Microsoft.AspNetCore.Http
@using Newtonsoft.Json
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@inject IHttpContextAccessor HttpContextAccessor
@{
    var session = HttpContextAccessor.HttpContext.Session;
    var listJson = session.GetString("ApplicationSettingList");
    var applicationSetting = !string.IsNullOrEmpty(listJson)
        ? JsonConvert.DeserializeObject<ApplicationSetting>(listJson)
        : null;
   var listprojectJson = session.GetString("projectdetail");
   var projectdetails = !string.IsNullOrEmpty(listprojectJson)
        ? JsonConvert.DeserializeObject<ProjectDetails>(listprojectJson)
        : null;
}
@model Entities.ApplicationSetting

@{
    Project project = new Project();
    //ApplicationSetting setting = new ApplicationSetting();
